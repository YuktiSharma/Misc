#include <iostream>
#include<string.h>
using namespace std;

int main() {
	// your code goes here
	char s1[50]={'a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z'};
	int count1[26]={0};
	    for(int i=0;i<26;i++)
	    {
	        count1[s1[i]-'a']++;
	    }
	    
	for(int k=0;k<26;k++)
	{
	    if(count1[k]==0)
	    {
	        break;
	        cout<<"NOT PANGRAM";
	    }
	    
	   }
	   cout<<"PANGRAM";
	    
	
	return 0;
}
