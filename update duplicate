#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int arr[100];
	int i,j,n;
	cin>>n;
	for(i=0;i<n;i++)
	{
	    cin>>arr[i];
	}
	for(int i=0;i<n;i++)
	{
	    j=i+1;
	    if(arr[i]==arr[j])
	    {
	        arr[j]= arr[i]+1;
	    }
	}
	for(i=0;i<n;i++)
	{
	    cout<<arr[i];
	}
	return 0;
}
