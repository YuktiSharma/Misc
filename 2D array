#include<stdio.h>
void main()

{
	int a[10][10],n,i,j,prod=1;
	printf("enter the size of matrix:\n");
	scanf("%d",&n);
	printf("enter the elements:\n");

	//to get elements for two-d array	
	for(i=0;i<n;i++)
	{
		for(j=0;j<n;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}

	//to print elements of two-d array	
	for(i=0;i<n;i++)
	{
		for(j=0;j<n;j++)
		{
			printf("%d\t",a[i][j]);
		}
	
	printf("\n");
	}
	
	//to assign the matrix to other
	int b[10][10];
	for(i=0;i<n;i++)
	{
		for(j=0;j<n;j++)
		{
			b[i][j]=a[i][j];
		}
	}

	/*
	//to get the lower triangle matrix
	for(j=0;j<n-1;j++)
	{
		for(i=j+1;i<n;i++)
		{
			printf("%d\t",a[i][j]);
		}
		printf("\n");
	}
	*/

	//calculation
	for(j=0;j<n-1;j++)
	{
		for(i=j+1;i<n;i++)
		{
			for(k=0;k<n;k++)
			{
			(b[i-1][k]/b[i-1][j])*b[i][j]
		
			}
		}
	}
	
	//to find product of diagonal elements
	for(i=0;i<n;i++)
	{
		prod=prod*a[i][i];
	}
	printf("the product is: %d\n",prod);

}			
